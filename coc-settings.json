{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "html", "lua", "json"],
  "emmet.includeLanguages": {
    "vue": "css"
  },
  "go.goplsArgs": ["-remote=auto", "-logfile", "/tmp/gopls.log"],
  "go.goplsOptions": {
    "usePlaceholders": true,
    "gofumpt": false
  },
  "list.maxPreviewHeight": 38,
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "java.jdt.ls.vmargs": "-javaagent:/home/essay/Library/java/lombok.jar",
  "explorer.width": 30,
  "explorer.file.showHiddenFiles": true,
  "stylua.styluaPath": "/usr/bin/stylua",
  "cSpell.userWords": [
  ],
  "Lua.telemetry.enable": true,
  "notification.statusLineProgress": false
}
